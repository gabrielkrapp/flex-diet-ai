generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id        String  @id @default(uuid())
  firstName String
  lastName  String
  dateOfBirth String
  height     Float
  weight     Float
  email      String  @unique
  password   String
  vegan      Boolean
  biotipo    String
  diabetes   Boolean
  lactose    Boolean
  gluten     Boolean
}
